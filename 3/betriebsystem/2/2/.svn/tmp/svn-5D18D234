#include <stdint.h>
#include <stdbool.h>
#include <unistd.h>

int main(int argc, char *argv[])
{
	printf("Kind_1 ...\n");
	printf("Kind_1 : %d | Elternprozess : %d\n", (int)getpid(), (int)getppid());
	
	sleep(10);
	printf("Kind1 Ende\n");

	return EXIT_SUCCESS;
}
